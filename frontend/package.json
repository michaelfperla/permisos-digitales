{
  "name": "permisos-digitales-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:admin": "vite --config vite.admin.config.ts",
    "build": "tsc -b && vite build",
    "build:admin": "tsc -b && vite build --config vite.admin.config.ts",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:admin": "vite preview --config vite.admin.config.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --reporter=verbose src/**/*.test.{ts,tsx}",
    "test:integration": "vitest run --reporter=verbose src/test/integration/**/*.test.{ts,tsx}",
    "test:admin": "vitest run --reporter=verbose src/admin/**/*.test.{ts,tsx}",
    "test:client": "vitest run --reporter=verbose src/{components,pages,services,hooks,utils}/**/*.test.{ts,tsx}",
    "test:ci": "vitest run --coverage --reporter=verbose",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\" \"*.{js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,md}\" \"*.{js,json,md}\""
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@tanstack/react-query": "^5.74.7",
    "@types/uuid": "^10.0.0",
    "axios": "^1.8.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.4",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.5.1",
    "uuid": "^11.1.0",
    "zod": "^3.25.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.15.24",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.5.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.3.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.5",
    "vitest": "^3.1.4"
  }
}
